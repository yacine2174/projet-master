{
  "name": "cybrix-solutions",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://projet-master-90u5.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://projet-master-90u5.onrender.com/api",
    "VITE_FRONTEND_URL": "https://cybrix-solutions.vercel.app"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}
