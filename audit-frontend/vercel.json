{
  "name": "cybrix-solutions",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://projet-master-90u5.onrender.com/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://projet-master-90u5.onrender.com/api",
    "VITE_FRONTEND_URL": "https://cybrix-solutions.vercel.app"
  }
}
